<div class="form">
  <h1>New post</h1> 
  <%= form_with model: @post do |form|%>
    <div>
      <%= form.label :title%> <br>
      <%= form.text_field :title%>
    </div>

    <div>
      <%= form.label :text%> <br>
      <%= form.text_area :text%>
    </div>

    <%= form.submit%>
  <%end%>
</div>
